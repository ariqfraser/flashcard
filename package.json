{
    "name": "flashcard",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --hmr --open",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "jest --coverage",
        "prepare": "husky install",
        "format": "prettier --write .",
        "cspell": "cspell \"**/*\" --no-progress",
        "lint-staged": "lint-staged",
        "pre-deploy": "ng build --configuration production",
        "deploy": "npm run pre-deploy && firebase deploy --project flashcard-c07be --only hosting:flashcard-c07be"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^20.2.0",
        "@angular/compiler": "^20.2.0",
        "@angular/core": "^20.2.0",
        "@angular/fire": "^20.0.1",
        "@angular/forms": "^20.2.0",
        "@angular/platform-browser": "^20.2.0",
        "@angular/router": "^20.2.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular/build": "^20.2.2",
        "@angular/cli": "^20.2.2",
        "@angular/compiler-cli": "^20.2.0",
        "@types/jest": "^30.0.0",
        "cspell": "^9.2.1",
        "firebase": "^11.10.0",
        "firebase-tools": "^14.15.2",
        "husky": "^9.1.7",
        "jest": "^30.1.3",
        "jest-preset-angular": "^15.0.0",
        "lint-staged": "^16.1.6",
        "prettier": "^3.6.2"
    }
}
